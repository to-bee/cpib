program FloatExamplesValid()
global
	fun example() returns var f:float
	local
		var f1:float;
		var f2:float;
		var f3:float;
		var f4:float;
		var f5:float;
		var f6:float;
	do
		f1 init := 4.2;
		f2 init := 2.0;
		f3 init := 1'3.4;
		f4 init := NEGATIVE_INFINITY;
		f5 init := POSITIVE_INFINITY;
		f6 init := NaN;
		f init := 0.0
	end fun
do	
	var f:float;
	f init := example();
end program