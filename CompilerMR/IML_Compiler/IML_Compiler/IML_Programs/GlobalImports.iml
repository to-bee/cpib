program GI()
global
	var x1:int32;
	var x2:int32;
	var x3:int32;
	var x4:int32;

	proc Test()
	global in var x1, inout var x2, out var x3
	local
		var f1:float
	do
		f1 init := 12.3;
		debugout f1;
		debugout x1;
		debugout x2;
		debugout x3 init;
		f1 := f1 * [float]2;
		x1 := x1 * 2;
		x2 := x2 * 2;
		x3 := x3 * 2;
		debugout f1;
		debugout x1;
		debugout x2;
		debugout x3
	endproc

do
	x1 init := 1;
	x2 init := 2;
	x3 init := 3;
	x4 init := 4;
	call Test();
	debugout x1;
	debugout x2;
	debugout x3;
	debugout x4
endprogram